{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run cached-download",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "LOGLEVEL": "verbose"
      },
      "program": "${workspaceFolder}/src/bin/run-once.ts",
      "args": [
        "--downloader=cached",
        "--uploader=console",
      ],
      "type": "pwa-node",
      "outFiles": [
        "${workspaceFolder}/out/**/*.js",
        "!**/node_modules/**"
      ]
    },
    {
      "name": "Run cached-download/scorer",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "LOGLEVEL": "verbose"
      },
      "program": "${workspaceFolder}/src/bin/run-once.ts",
      "args": [
        "--downloader=cached",
        "--uploader=console",
        "--cached-fns=scorer.dat",
      ],
      "type": "pwa-node",
      "outFiles": [
        "${workspaceFolder}/out/**/*.js",
        "!**/node_modules/**"
      ]
    },
    {
      "name": "Downloader",
      "program": "${workspaceFolder}/src/bin/download.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node",
      "outFiles": [
        "${workspaceFolder}/out/**/*.js",
        "!**/node_modules/**"
      ]
    }
  ]
}