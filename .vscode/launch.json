  "version": "0.2.0",
  "configurations": [
    {
      "name": "Dry-run",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/bin/run-once.ts",
      "args": [
        "fast"
      ],
      "type": "pwa-node"
    },
    {
      "name": "Dry-run 3x",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/bin/run-3x.ts",
      "type": "pwa-node"
    },
    {
      "name": "Investigate Data",
      "program": "${workspaceFolder}/src/bin/investigate.ts",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    }
  ]
}